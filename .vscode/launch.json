    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'codt-cli'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=codt-cli",
                    "--package=codt-cli"
                ],
                "filter": {
                    "name": "codt-cli",
                    "kind": "bin"
                },
                "cwd": "${workspaceFolder}/codt"
            },
            "args": [
                "-f",
                "../datasets/classification/bank.csv", "-d", "2", "-s", "bfs-lds", "accuracy"
            ],
            "cwd": "${workspaceFolder}/codt",
            "relativePathBase": "${workspaceFolder}/codt",
        }
    ]
}